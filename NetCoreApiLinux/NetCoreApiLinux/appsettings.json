{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [ "Serilog.Sinks.Seq" ],
        "WriteTo": [
            {
                "Name": "Seq",
                "Args": { "serverUrl": "http://seq:5341" }
            }
        ]
    }
}
